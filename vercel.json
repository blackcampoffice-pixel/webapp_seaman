{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "ALLOWALL" },
        { "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://*.telegram.org https://web.telegram.org https://web.telegram.org/a" }
        ]
    }
  ]
}
